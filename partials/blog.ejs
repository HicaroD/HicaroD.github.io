<%- include(metaconfig.paths.layouts.start); %>
<main id="blog">
  <% posts.forEach(function(post){ -%>
  <div class="post">
    <h2 class="title">
      <a class="link" href="<%= post.title.link %>"><%= post.title.name %></a>
    </h2>
    <p><%= post.description %></p>
    <p class="date"><%= post.createdAt %></p>
    <div class="tags">
      <% post.tags.forEach(function(tag){ -%>
      <p><a class="link" href="#">#<%= tag %></a></p>
      <% }); -%>
    </div>
  </div>
  <% }); -%>
</main>
<%- include(metaconfig.paths.layouts.end); %>
